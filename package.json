{
  "name": "gitbook-plugin-page-footer",
  "version": "3.0.8",
  "description": "a gitbook-plugin for generating footer",
  "engines": {
    "gitbook": ">= 2.4.5"
  },
  "main": "./lib/index.js",
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aleen42/gitbook-footer.git"
  },
  "keywords": [
    "gitbook",
    "gitbook-plugin",
    "footer"
  ],
  "author": "aleen42 <aleen42@vip.qq.com> (http://aleen42.github.io/pc.html)",
  "contributors": [
    "aleen42 <aleen42@vip.qq.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aleen42/gitbook-footer/issues"
  },
  "homepage": "https://github.com/aleen42/gitbook-footer#readme",
  "dependencies": {
    "moment": "^2.10.6"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  },
  "gitbook": {
    "properties": {
      "description": {
        "type": "string",
        "default": "modified at",
        "title": "Description for your modified motion"
      },
      "author": {
        "type": "string",
        "default": "Aleen",
        "title": "Author"
      },
      "wisdom": {
        "type": "string",
        "default": "More than a coder, more than a designer",
        "title": "Windom"
      },
      "format": {
        "type": "string",
        "default": "YYYY-MM-DD HH:mm:ss",
        "title": "Time format"
      },
      "copyright": {
        "type": "string",
        "default": "Copyright Â© aleen42",
        "title": "Description for your Copyright"
      },
      "timeColor": {
        "type": "string",
        "default": "#666",
        "title": "Default color for modifed time"
      },
      "copyrightColor": {
        "type": "string",
        "default": "#666",
        "title": "Default color for Copyright"
      },
      "utcOffset": {
        "type": "string",
        "default": "8",
        "title": "Timezone",
        "min": "-16",
        "max": "16"
      },
      "qrcode": {
        "type": "boolean",
        "default": true,
        "title": "QRCode"
      },
      "baseUri": {
        "type": "string",
        "default": "https://aleen42.gitbooks.io/personalwiki/content/",
        "description": "BaseUri of the first page of your book"
      }
    }
  }
}
